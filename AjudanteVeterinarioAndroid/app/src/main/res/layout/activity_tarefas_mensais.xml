<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sarah.ajudanteveterinarioandroid.Activity.TarefasMensaisActivity">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButtonVoltarMenuTM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:tint="@android:color/background_light"
        app:fabSize="mini"
        app:srcCompat="@drawable/ic_keyboard_backspace_black_48dp"
        tools:layout_editor_absoluteX="413dp"
        tools:layout_editor_absoluteY="683dp"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/ListViewTarefasMensais"
        android:layout_marginBottom="25dp" />

    <ListView
        android:layout_width="464dp"
        android:layout_height="671dp"
        android:id="@+id/ListViewTarefasMensais"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="52dp"
        android:layout_above="@+id/floatingActionButtonVoltarMenuTM"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
