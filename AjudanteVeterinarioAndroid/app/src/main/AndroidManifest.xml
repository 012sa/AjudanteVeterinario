<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sarah.ajudanteveterinarioandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cow_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.ListarFazendaActivity" />
        <activity android:name=".Activity.TarefasMensaisActivity" />
        <activity android:name=".Activity.CadastrarFazendaActivity" />
        <activity android:name=".Activity.PerfilFazendaActivity" />
        <activity android:name=".Activity.ListarAnimaisActivity"></activity>
    </application>

</manifest>