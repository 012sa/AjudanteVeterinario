        android:allowBackup="true"
        android:icon="@drawable/cowicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.Activities.MainActivity" />
        <activity android:name=".main.Activities.ListarFazendasActivity" />
        <activity android:name=".main.Activities.TarefasMensaisActivity" />
        <activity android:name=".main.Activities.CadastrarFazendaActivity" />
        <activity android:name=".main.Activities.PerfilFazendaActivity"></activity>
    </application>

</manifest>